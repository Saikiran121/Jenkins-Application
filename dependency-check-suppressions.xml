<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!-- Suppress MongoDB server CPE incorrectly mapped to Node driver -->
    <suppress>
        <notes><![CDATA[
        Suppressing vulnerabilities for the mongodb npm package as they are often 
        incorrectly mapped to the MongoDB server CPE.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/mongodb@.*$</packageUrl>
        <cpe regex="true">cpe:2.3:a:mongodb:mongodb:.*</cpe>
    </suppress>

    <!-- Suppress time-span CVE -->
    <suppress>
        <cve>CVE-2017-16115</cve>
    </suppress>

</suppressions>
