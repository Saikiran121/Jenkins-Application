<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!-- Suppress ALL vulnerabilities for the mongodb npm package -->
    <suppress>
        <notes><![CDATA[
        Incorrectly mapped to MongoDB Server vulnerabilities.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/mongodb@.*$</packageUrl>
    </suppress>

    <!-- Suppress formidable vulnerabilities -->
    <suppress>
        <notes><![CDATA[
        Suppressing formidable CVE-2025-46653.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/formidable@.*$</packageUrl>
        <cve>CVE-2025-46653</cve>
    </suppress>

    <!-- Correcting time-span suppression -->
    <suppress>
        <notes><![CDATA[
        Suppressing time-span vulnerability (CVE-2017-16115).
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/time-span@.*$</packageUrl>
        <vulnerabilityName>CVE-2017-16115</vulnerabilityName>
    </suppress>

</suppressions>
