<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!-- Suppress MongoDB server CVEs incorrectly mapped to Node driver -->
    <suppress>
        <packageUrl regex="true">pkg:npm/mongodb@7\.0\.0</packageUrl>
        <cpe regex="true">cpe:2\.3:a:mongodb:mongodb:.*</cpe>
    </suppress>

    <!-- Suppress legacy dev dependency time-span -->
    <suppress>
        <packageUrl regex="true">pkg:npm/time-span@5\.1\.0</packageUrl>
        <cve>CVE-2017-16115</cve>
    </suppress>

</suppressions>
